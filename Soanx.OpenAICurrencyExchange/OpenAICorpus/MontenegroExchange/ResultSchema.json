{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "Id": {
        "type": "number"
      },
      "Cities": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "Offers": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "Cities": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "Sell": {
              "type": "object",
              "properties": {
                "Currencies": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "enum": [ "unknown", "USDT", "USDC", "BUSD", "EUR", "USD", "UAH", "RUB" ]
                  }
                },
                "Banks": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "AmountMin": {
                  "type": "number"
                },
                "AmountMax": {
                  "type": "number"
                }
              },
              "required": [ "Currencies" ]
            },
            "Buy": {
              "type": "object",
              "properties": {
                "Currencies": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "enum": [ "unknown", "USDT", "USDC", "BUSD", "EUR", "USD", "UAH", "RUB" ]
                  }
                },
                "Banks": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "RateMin": {
                  "type": "number"
                },
                "RateMax": {
                  "type": "number"
                }
              },
              "required": [ "Currencies" ]
            },
            "SellToBuyRate": {
              "type": "object",
              "properties": {
                "RateMin": {
                  "type": "number"
                },
                "RateMax": {
                  "type": "number"
                }
              }
            }
          },
          "required": [ "Sell", "Buy", "SellToBuyRate" ]
        }        
      },
      "NotMatched": {
        "type": "boolean"
      }
    },
    "required": [ "Id" ]
  }
}